server:
    port: 9092
    context-path: /cloud-provider-002

druid:
    url: jdbc:mysql://192.168.126.147:3306/boss?useUnicode=true&amp;characterEncoding=UTF-8&amp;zeroDateTimeBehavior=convertToNull
    driver-class-name: com.mysql.jdbc.Driver
    username: root
    password: 111111
    initial-size: 5
    min-idle: 1
    max-active: 100
    test-on-borrow: true

mybatis:
    type-aliases-package: com.cloud.model
    mapper-locations: classpath:mapper/*.xml

mapper:
    mappers:
        - com.cloud.database.BaseMapper
    not-empty: false
    identity: MYSQL

pagehelper:
    helperDialect: mysql
    reasonable: true
    supportMethodsArguments: true
    params: count=countSql

eureka:
  client:
    service-url:
      defaultZone: http://localhost:8761/eureka
  healthcheck:
   enable: true
  instance:
    prefer-ip-address: true #指定显示ip
    instance-id: ${spring.cloud.client.hostname}:${spring.application.name}:${spring.application.instance_id:${server.port}}
